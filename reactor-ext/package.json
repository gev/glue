{
  "name": "reactor-lang",
  "displayName": "Reactor Language Support",
  "description": "Syntax highlighting for Reactor language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "reactor",
        "aliases": [
          "Reactor",
          "reactor"
        ],
        "extensions": [
          ".rct",
          ".reactor"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "reactor",
        "scopeName": "source.reactor",
        "path": "./syntaxes/reactor.tmLanguage.json"
      }
    ]
  }
}
